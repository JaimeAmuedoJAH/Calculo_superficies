<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Calculo_superficies"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".rectangleArea"
                  android:exported="true">
        </activity>
        <activity android:name=".squareArea"
            android:exported="true">
        </activity>
        <activity android:name=".ovalArea"
            android:exported="true">
        </activity>
        <activity android:name=".circleArea"
            android:exported="true">
        </activity>
        <activity android:name=".triangleArea"
            android:exported="true">
        </activity>
        <activity android:name=".rhombusArea"
            android:exported="true">
        </activity>
        <activity android:name=".pentagonArea"
            android:exported="true">
        </activity>
        <activity android:name=".hexagonArea"
            android:exported="true">
        </activity>
    </application>

</manifest>